<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:id="@+id/widget29"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow android:id="@+id/widget35" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:padding="5px"
		android:orientation="horizontal">
		<EditText android:id="@+id/cname" android:layout_width="200px"
			android:layout_height="wrap_content" android:text=""
			android:textSize="20sp">
		</EditText>
		<Button android:id="@+id/ok_button" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Add"
			android:textSize="20sp" android:typeface="serif" android:textStyle="bold">
		</Button>
	</TableRow>
	<ListView android:id="@+id/list" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:padding="5px">
	</ListView>
</TableLayout>
